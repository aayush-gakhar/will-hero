<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.willhero.MainController">
   <children>
      <ImageView fx:id="background" fitHeight="768.0" fitWidth="1024.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/background.png" />
      </ImageView>
      <Group layoutX="10.0" layoutY="10.0">
         <ImageView fx:id="btnMusic" fitHeight="55.0" fitWidth="55.0" onMouseClicked="#onMusicButtonClick" pickOnBounds="true" preserveRatio="true">
            <Image url="@assets/btnmusic0.png" />
         </ImageView>
         <ImageView fx:id="btnSound" fitHeight="55.0" fitWidth="55.0" layoutX="70.0" onMouseClicked="#onSoundButtonClick" pickOnBounds="true" preserveRatio="true">
            <Image url="@assets/btnsound0.png" />
         </ImageView>
      </Group>
      <ImageView fx:id="hero" fitHeight="77.0" fitWidth="75.0" layoutX="121.0" layoutY="337.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/hero.png" />
      </ImageView>
      <ImageView fitHeight="120.0" fitWidth="202.0" layoutX="60.0" layoutY="520.0" pickOnBounds="true">
         <Image url="@assets/ground1.png" />
      </ImageView>
      <ImageView fitHeight="181.0" fitWidth="279.0" layoutX="371.0" layoutY="118.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/gamename.png" />
      </ImageView>
      <ImageView fx:id="btnStartGame" fitHeight="66.0" fitWidth="66.0" layoutX="438.0" layoutY="331.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/btnresume.png" />
      </ImageView>
      <ImageView fx:id="btnLoadGame" fitHeight="66.0" fitWidth="66.0" layoutX="522.0" layoutY="331.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/btnload.png" />
      </ImageView>
   </children>
</AnchorPane>
