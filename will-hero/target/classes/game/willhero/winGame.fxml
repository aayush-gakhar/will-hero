<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Group?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="game.willhero.WinGameController">
   <ImageView fx:id="background" fitHeight="768.0" fitWidth="1024.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@assets/background.png" />
   </ImageView>
   <Group fx:id="clouds">
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="500.0" layoutY="100.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/cloud1.png" />
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="750.0" layoutY="150.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/cloud0.png" />
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="80.0" layoutY="200.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/cloud2.png" />
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="200.0" layoutY="80.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/cloud0.png" />
      </ImageView>
   </Group>
   <Group layoutX="10.0" layoutY="10.0">
      <ImageView fx:id="btnMusic" fitHeight="55.0" fitWidth="55.0" onMouseClicked="#onMusicButtonClick" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/btnmusic0.png" />
      </ImageView>
      <ImageView fx:id="btnSound" fitHeight="55.0" fitWidth="55.0" layoutX="70.0" onMouseClicked="#onSoundButtonClick" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/btnsound0.png" />
      </ImageView>
      <ImageView fitHeight="55.0" fitWidth="55.0" layoutX="160.0" onMouseClicked="#onBackButtonClick"
                 pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/btnback.png"/>
      </ImageView>
   </Group>
   <ImageView fx:id="light" fitHeight="200.0" fitWidth="200.0" layoutX="419.0" layoutY="240.0" pickOnBounds="true"
              preserveRatio="true">
      <Image url="@assets/light.png"/>
   </ImageView>
   <ImageView fx:id="hero" fitHeight="49.0" fitWidth="65.0" layoutX="505.0" layoutY="310.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@assets/hero.png" />
   </ImageView>
   <Group layoutX="60.0" layoutY="520.0">
      <ImageView fitHeight="120.0" fitWidth="178.0" pickOnBounds="true" preserveRatio="true">
         <Image url="@assets/ground0.png"/>
      </ImageView>
      <ImageView fitHeight="226.0" fitWidth="178.0" layoutX="751.0" layoutY="-100.0" pickOnBounds="true"
                 preserveRatio="true">
         <Image url="@assets/groundmenu0.png"/>
      </ImageView>
      <ImageView fitHeight="222.0" fitWidth="202.0" layoutX="296.0" layoutY="-59.0" pickOnBounds="true"
                 preserveRatio="true">
         <Image url="@assets/groundmenu2.png"/>
      </ImageView>
      <ImageView fitHeight="210.0" fitWidth="187.0" layoutX="497.0" layoutY="-54.0" pickOnBounds="true">
         <Image url="@assets/groundmenu.png"/>
      </ImageView>
   </Group>
   <Text fill="#0fab1c" layoutX="360.0" layoutY="250.0" strokeType="OUTSIDE" strokeWidth="0.0" text="You Won!!">
      <font>
         <Font size="72.0" />
      </font>
   </Text>
</AnchorPane>
